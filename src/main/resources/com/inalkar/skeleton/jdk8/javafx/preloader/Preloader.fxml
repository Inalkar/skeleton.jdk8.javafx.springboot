<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="190.0" prefWidth="492.0" styleClass="root" stylesheets="@Preloader.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0" spacing="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox prefHeight="100.0" prefWidth="200.0" spacing="20.0" VBox.vgrow="ALWAYS">
               <children>
                  <VBox prefWidth="100.0" HBox.hgrow="ALWAYS">
                     <children>
                        <VBox VBox.vgrow="ALWAYS">
                           <children>
                              <HBox prefWidth="200.0" spacing="5.0">
                                 <children>
                                    <Label alignment="BOTTOM_LEFT" text="Software version">
                                    </Label>
                                    <Label fx:id="versionLabel" text="%app.version" />
                                 </children>
                                 <padding>
                                    <Insets bottom="16.0" top="16.0" />
                                 </padding>
                              </HBox>
                           </children>
                           <padding>
                              <Insets top="-10.0" />
                           </padding>
                        </VBox>
                        <VBox prefWidth="100.0" spacing="5.0" VBox.vgrow="NEVER">
                           <children>
                              <Label fx:id="progressLabel" maxWidth="1.7976931348623157E308" text="Loading..." />
                              <ProgressBar fx:id="progressBar" maxHeight="11.0" maxWidth="1.7976931348623157E308" minHeight="11.0" prefHeight="11.0" progress="0.35" VBox.vgrow="NEVER" />
                           </children>
                        </VBox>
                     </children>
                  </VBox>
               </children>
            </HBox>
            <HBox alignment="BASELINE_LEFT" prefWidth="200.0" spacing="5.0" VBox.vgrow="NEVER">
               <children>
                  <Label maxWidth="1.7976931348623157E308" text="Copyright" HBox.hgrow="NEVER" />
                  <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="%app.copyright-year" />
                  <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="%app.copyright-company" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
